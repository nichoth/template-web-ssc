{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false
  },
  "rules": {
    "operator-linebreak": ["error", "after"],
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 1,
        "maxEOF": 1
      }
    ],
    "indent": ["error", 4, {
      "SwitchCase": 1,
      "ignoredNodes": ["TemplateLiteral *"]
    }],
    "comma-dangle": "off",
    "quotes": ["error", "single", { "allowTemplateLiterals": true }]
  }
}
